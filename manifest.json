{
  "action": {
    "default_icon": {
      "128": "icons/icon-128.png",
      "16": "icons/icon-16.png",
      "48": "icons/icon-48.png"
    },
    "default_popup": "popup/popup.html"
  },
  "background": {
    "service_worker": "background/background.js"
  },
  "content_scripts": [
    {
      "all_frames": true,
      "js": ["content/content-script.js"],
      "matches": [
        "https://www.cityexperiences.com/*",
        "https://*.cityexperiences.com/*",
        "https://*.embedsocial.com/*",
        "https://embedsocial.com/*",
        "https://*.citycruises.com/*",
        "https://citycruises.com/*",
        "https://*.ticketcounter.eu/*",
        "https://ticketcounter.eu/*"
      ],
      "run_at": "document_idle"
    }
  ],
  "description": "Automatise la recherche de cr√©neau 10:10 AM sur City Cruises Alcatraz",
  "host_permissions": [
    "https://www.cityexperiences.com/*",
    "https://*.cityexperiences.com/*",
    "https://*.embedsocial.com/*",
    "https://embedsocial.com/*",
    "https://*.citycruises.com/*",
    "https://citycruises.com/*",
    "https://*.ticketcounter.eu/*",
    "https://ticketcounter.eu/*"
  ],
  "manifest_version": 3,
  "name": "Booking Automator",
  "permissions": ["scripting", "storage", "activeTab", "tabs"],
  "version": "1.0.1"
}
